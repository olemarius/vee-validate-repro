{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ],
      "cache": true
    },
    "preview": {
      "dependsOn": [
        "^preview"
      ],
      "outputs": [
        "dist/**"
      ],
      "cache": true
    },
    "lint": {},
    "dev": {
      "cache": false
    }   
  }
}
